{
  "SystemShortcutBehavior": 3,
  "ShortcutCustomizationAllowed": true,
  "HomepageIsNewTabPage": false,
  "VideoCaptureAllowedUrls": ["https://[*.]google.com/"],
  "ExtensionSettings": {
    "{{ local_extension_show_tab_numbers }}": {
      "allowed_permissions": [
        "contextMenus",
        "scripting",
        "storage",
        "tabGroups",
        "tabs"
      ],
      "file_url_navigation_allowed": true
    },
    "{{ local_extension_new_tab_redirect }}": {
      "allowed_permissions": ["favicon", "storage"],
      "file_url_navigation_allowed": true
    },
    "{{ local_extension_promnesia }}": {
      "toolbar_pin": "force_pinned",
      "toolbar_state": "force_shown"
    },
    "{{ extension_monolith }}": {
      "toolbar_pin": "force_pinned",
      "toolbar_state": "force_shown"
    },
    "{{ local_hypothesis_extension }}": {
      "toolbar_pin": "force_pinned",
      "toolbar_state": "force_shown",
      "file_url_navigation_allowed": true,
      "allowed_permissions": ["scripting", "storage", "tabs"]
    },
    "{{ local_extension_vimium }}": {
      "file_url_navigation_allowed": true,
      "allowed_permissions": [
        "tabs",
        "bookmarks",
        "history",
        "storage",
        "sessions",
        "notifications",
        "scripting",
        "favicon",
        "webNavigation",
        "search"
      ]
    }
  }
}
